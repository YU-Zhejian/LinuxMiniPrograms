#!/usr/bin/env bash
# LIBMAN V1
function getuser() {
	if [ -n "${USER:-}" ] ;then
		printf "${USER}"
		return
	elif [ -n "${USERNAME:-}" ] ;then
		printf "${USERNAME}"
		return
	else
		return 1
	fi
}
function getcorenumber(){
	"${mycat}" /proc/cpuinfo | "${mygrep}" '^processor\s: ' | wc -l | awk '{print $1}'
}
