#!/usr/bin/env bash
# LIBDO_MONITOR V1
set -ue

DN="$(readlink -f "$(dirname "${0}")")"
. "${DN}"/../etc/path.sh
. "${DN}"/../lib/libstr
function __exit() {
	kill -9 ${$}
}
trap "__exit" SIGINT
if [ "${mypython}" != "ylukh" ]; then
	exec "${mypython}" "${DN}"/exec/libdo_monitor.py "${@}"
else
	errh "Python not found"
fi
