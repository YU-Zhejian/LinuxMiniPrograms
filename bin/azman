#!/usr/bin/env bash
# AZMAN V3P2
DN="$(readlink -f "$(dirname "${0}")")"
exec 1>&2
ISAUTOZIP=false
. "${DN}"/exec/libautozip.sh
echo -e "\033[33mYuZJLab AutoZipManager"
echo -e "Copyright (C) 2020 YU Zhejian\033[0m"
ppopt "${@}"
# ============ Possess arguments ============
azcmd=${STDS[0]:-}
unset STDS[0]
if [ -z "${azcmd}" ]; then
	autozipck
	exit 0
elif [ -f "${DN}"/exec/azman_"${azcmd}".sh ];then
	. "${DN}"/exec/azman_"${azcmd}".sh
else
	echo -e "\033[31mERROR: subcommand '${azcmd}' invalid! \033[0m"
	exit 1
fi
