#!/usr/bin/env bash
# AZMAN V3P2
DN="$(readlink -f "$(dirname "${0}")")"
exec 1>&2
ISAUTOZIP=false
. "${DN}"/exec/libautozip.sh
infoh "YuZJLab AutoZipManager"
infoh "Copyright (C) 2020 YU Zhejian"
ppopt "${@}"
# ============ Possess arguments ============
azcmd=${STDS[0]:-}
unset STDS[0]
if [ -z "${azcmd}" ]; then
	autozipck
	exit 0
elif [ -f "${DN}"/exec/azman_"${azcmd}".sh ];then
	. "${DN}"/exec/azman_"${azcmd}".sh
else
	errh "subcommand '${azcmd}' invalid!"
fi
