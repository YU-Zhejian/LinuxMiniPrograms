#!/usr/bin/env bash
# LIBDOMAN V6
set -ue
DN="$(readlink -f "$(dirname "${0}")")"
. "${DN}"/../etc/path.conf
. "${DN}"/../lib/libstr
infoh "YuZJLab LibDO Manager"
infoh "Copyright (C) 2020 YU Zhejian"
if [ "${mypython}" != "ylukh" ]; then "${mypython}" "${DN}"/exec/libdoman.py "${@}"; else . "${DN}"/exec/libdoman.sh; fi
