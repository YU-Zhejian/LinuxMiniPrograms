#!/usr/bin/env bash
# PST v1
set -ue
DN="$(readlink -f "$(dirname "${0}")")"
. "${DN}"/../lib/libstr
. "${DN}"/../etc/path.sh
for fn in "${DN}"/exec/pst.co "${DN}"/exec/pst.co.exe;do
	if [ -e "${fn}" ];then
		exec "${fn}"
	fi
done
if [ "${mypython}" != "ylukh" ]; then exec "${mypython}" "${DN}"/exec/pst.py "${@}"; else errh "No python found!"; fi
